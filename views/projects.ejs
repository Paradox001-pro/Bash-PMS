



<%- include('./partials/header') -%>
<%- include('./partials/messages') -%>

<div class="container">

    <h3 class="h2 display-3 text-center text-info">Projects</h3>
    <div class="row">
        <div class="col">
            <% if(task){  %>
            <% task.forEach(task => { %>
            <ul class="list-group">
                <a href="projects/<%= task._id %>" class="text-dark">
                    <li class="list-group-item my-1">
                        <%= task.projectName %>
                    </li>
                </a>
            </ul>
            <% }) %>
            <% }else{ %>
            <h2>Oops no Project yet</h2>
            <%}%>
        </div>
    </div>
</div>

<%- include('./partials/footer') -%>